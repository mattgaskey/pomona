{% embed '@clamp' with { variant: 'content' } %}
{% block main_content %}
<div class="c-container">
  {% include "@breadcrumb" %}
  <h1 class="font-serif text-6xl mb-30">Admissions</h1>
</div>

<div class="c-container-lg  mx-auto">
  {% include '@slideshow' with { variant: 'presentation', pattern: "@presentation" }  %}
  {% include '@two-sections-landing-page' %}
</div>

<div class="bg-gray-200 bg-pttrn-lt-grey pt-50 pb-100">
  <div class="c-container">
    <div class="grid-of-items overflow-hidden">
      <div class="md:flex md:-mr-25 md:flex-wrap md:items-stretch md:space-between">
        {% for contact in contacts  %}
          <div class="md:pr-25 flex-grow md:w-1/2 lg:w-1/3 flex flex-col justify-stretch">
            {% include '@page-contact' with { whom: contact.whom, link_title: contact.link_title } %}
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
</div>
{% endblock %}
{% endembed %}